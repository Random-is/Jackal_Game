<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="rootPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MenuView">
    <children>
        <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="312.0" spacing="5.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#selectGameType" text="SINGLE"/>
                        <Button mnemonicParsing="false" onAction="#selectGameType" text="MULTI"/>
                        <Button mnemonicParsing="false" onAction="#selectGameType" text="HOST"/>
                        <Button mnemonicParsing="false" onAction="#selectGameType" text="CONNECT"/>
                        <Button mnemonicParsing="false" onAction="#selectGameType" text="HOTSEAT"/>
                    </children>
                </HBox>
            </children>
        </StackPane>
    </children>
</AnchorPane>
